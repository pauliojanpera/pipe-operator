{
	"name": "@pauliojanpera/pipe-operator",
	"productName": "Pipe operator",
	"version": "0.0.4",
	"description": "A typed pipe operator",
	"author": {
		"name": "Pauli Ojanperä",
		"email": "pipe-operator@miettii.net"
	},
	"copyright": "Pauli Ojanperä",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/pauliojanpera/pipe-operator.git"
	},
	"type": "module",
	"module": "esm/index.js",
	"main": "cjs/index.js",
	"exports": {
		".": {
			"import": "./esm/index.js",
			"require": "./cjs/index.js"
		}
	},
	"scripts": {
		"dev": "pnpm verdaccio --listen 8798 --config ./verdaccio.yaml",
		"build": "shx rm -rf .dist ; tsc -p tsconfig.json && tsc -p tsconfig-cjs.json && node ./post-build.mjs && pnpm publish ./.dist --registry http://localhost:8798/ --access public --no-git-checks",
		"test": "NODE_OPTIONS=--experimental-vm-modules jest",
		"pub": "pnpm publish ./.dist --access public --publish-branch master",
		"pubdryrun": "pnpm publish ./.dist --access public --publish-branch master --dry-run --no-git-checks"
	},
	"keywords": [
		"pipe",
		"pipeline",
		"postfix",
		"operator",
		"typescript",
		"cjs",
		"esm",
		"es2015",
		"es6"
	],
	"devDependencies": {
		"@types/jest": "^27.0.2",
		"@types/node": "^16.11.0",
		"fs-extra": "^10.0.0",
		"jest": "^27.2.5",
		"jest-environment-jsdom": "^27.2.5",
		"shx": "^0.3.3",
		"ts-jest": "^27.0.6",
		"typescript": "^4.4.4"
	}
}
